{
  "name": "ireact-material-icons",
  "version": "0.0.1",
  "description": "google material icons",
  "main": "dist/ivue-material-plus.min.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ireact-material/ireact-material-icons"
  },
  "scripts": {
    "start": "react-scripts start",
    "build:dist:clean": "rimraf dist/ && yarn build:dist",
    "src:download": "node ./scripts/download.mjs",
    "src:icons": "cross-env UV_THREADPOOL_SIZE=64 node ./builder.mjs --output-dir src --svg-dir material-icons --renameFilter ./renameFilters/material-design-icons.mjs"
  },
  "dependencies": {
    "@babel/eslint-plugin": "^7.19.1",
    "@types/node": "^16.18.11",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/yargs": "^17.0.19",
    "cross-env": "^7.0.3",
    "cross-fetch": "^3.1.5",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-compat": "^4.0.0",
    "eslint-plugin-import": "^2.21.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-unicorn": "^45.0.0",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.0",
    "lodash": "^4.17.21",
    "mustache": "^4.2.0",
    "path": "^0.12.7",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0",
    "react-scripts": "5.0.1",
    "rimraf": "^4.0.4",
    "svgo": "^3.0.2",
    "typescript": "^4.9.4",
    "url": "^0.11.0",
    "web-vitals": "^2.1.4",
    "yargs": "^17.6.2"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "rome format --write",
    "*.{json,less,md}": "prettier --ignore-unknown --write"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
